{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "YC ToDo",
  "version": "0.1.0",
  "identifier": "dev.yc.todo",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "YC_ToDO",
        "width": 360,
        "height": 520,
        "resizable": false,
        "fullscreen": false,
        "decorations": false,
        "transparent": false,
        "visible": false,
        "alwaysOnTop": true,
        "skipTaskbar": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.icns"]
  }
}
